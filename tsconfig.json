{
  "compilerOptions": {
    "target": "es2016",  
    "jsx": "react",                                
    "module": "esnext",                              
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                     
    "skipLibCheck": true,                           
    "moduleResolution":"node",
    "paths": {
      "@/public/*": ["./public/*"],
      "@/*": ["./src/*"]
    },
  }
}
